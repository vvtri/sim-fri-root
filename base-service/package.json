{
  "name": "message-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register node_modules/typeorm/cli.js",
    "migration:generate": "npm run build && cross-var npm run typeorm migration:generate -- -d ./data-source.ts  ./migrations/$npm_config_name",
    "migration:up": "npm run build && npm run typeorm migration:run -- -d ./data-source.ts",
    "migration:down": "npm run build && npm run typeorm -- -d ./data-source.ts migration:revert",
    "migration:create": "cross-var npm run typeorm -- migration:create ./migrations/$npm_config_name"
  },
  "dependencies": {
    "@nestjs/common": "9.4.0",
    "@nestjs/config": "2.3.1",
    "@nestjs/core": "9.4.0",
    "@nestjs/jwt": "10.0.3",
    "@nestjs/passport": "9.0.3",
    "@nestjs/platform-express": "9.4.0",
    "@nestjs/swagger": "6.3.0",
    "@nestjs/typeorm": "9.0.1",
    "@sendgrid/mail": "7.7.0",
    "@types/bcryptjs": "2.4.2",
    "@vvtri/nestjs-kafka": "1.1.4",
    "bcryptjs": "2.4.3",
    "boolean": "3.2.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "common": "git+https://github.com/vvtri/sim-fri-common.git#develop",
    "dayjs": "1.11.7",
    "joi": "17.9.1",
    "jsonwebtoken": "9.0.0",
    "nestjs-typeorm-paginate": "4.0.3",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "pg": "8.10.0",
    "prettier-plugin-organize-imports": "3.2.2",
    "random-number-csprng": "1.0.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.0",
    "shared": "git+https://github.com/vvtri/sim-fri-shared.git#develop",
    "typeorm": "0.3.15",
    "typeorm-transactional": "0.4.1"
  },
  "devDependencies": {
    "@nestjs/cli": "9.4.0",
    "@nestjs/schematics": "9.1.0",
    "@nestjs/testing": "9.4.0",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/passport-jwt": "3.0.8",
    "@types/random-number-csprng": "1.0.0",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "cross-var": "1.1.0",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.2.1",
    "prettier": "2.8.7",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.0.4"
  }
}
